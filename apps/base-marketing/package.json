{
	"name": "@taaply/base-marketing",
	"version": "0.0.1",
	"private": true,
	"nx": {
		"targets": {
			"paraglide": {
				"command": "paraglide-js compile --project ./apps/base-marketing/project.inlang --outdir ./apps/base-marketing/src/paraglide"
			},
			"build": {
				"dependsOn": [
					"ui",
					"paraglide"
				],
				"options": {
					"assets": [
						{
							"input": "libs/shared/assets/src",
							"glob": "**/*",
							"output": "assets"
						},
						{
							"input": "libs/shared/assets/src/icons",
							"glob": "favicon.ico",
							"output": ""
						}
					]
				}
			}
		}
	},
	"devDependencies": {
		"@inlang/paraglide-js": "2.4.0",
		"@inlang/cli": "^3.0.0"
	},
	"scripts": {}
}
